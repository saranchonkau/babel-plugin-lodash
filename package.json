{
  "name": "babel-plugin-transform-lodash-import",
  "version": "3.3.4",
  "description": "Modular Lodash builds without the hassle.",
  "keywords": [
    "babel-plugin",
    "cherry-pick",
    "lodash",
    "modules"
  ],
  "author": "Graeme Yeates <megawac@gmail.com> (https://github.com/megawac)",
  "contributors": [
    "Graeme Yeates <megawac@gmail.com> (https://github.com/megawac)",
    "John-David Dalton <john.david.dalton@gmail.com>"
  ],
  "repository": "lodash/babel-plugin-lodash",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "prepublish": "npm run build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/helper-plugin-test-runner": "^7.24.7",
    "@babel/plugin-proposal-function-bind": "^7.18.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/plugin-syntax-typescript": "^7.25.4",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/types": "^7.25.2",
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/lodash": "^4.17.7",
    "@types/lodash-es": "^4.17.12",
    "babel-jest": "^29.7.0",
    "babel-plugin-lodash": "^3.3.2",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rollup": "^4.20.0",
    "ts-jest": "^29.2.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@babel/helper-module-imports": "^7.18.6"
  },
  "optionalDependencies": {
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21"
  }
}
